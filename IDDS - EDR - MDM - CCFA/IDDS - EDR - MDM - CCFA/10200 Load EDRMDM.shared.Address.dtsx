          <DTS:Variables />
          <DTS:ObjectData>
            <pipeline
              version="1">
              <components>
                <component
                  refId="Package\Load_Source_Data_Into_EDRStage\Upsert to EDRStage\Get CCFA Organization Location Data"
                  componentClassID="Microsoft.ManagedComponentHost"
                  contactInfo="Consumes data from SQL Server, OLE DB, ODBC, or Oracle, using the corresponding .NET Framework data provider. Use a Transact-SQL statement to define the result set. For example, extract data from SQL Server with the .NET Framework data provider for SQL Server.;Microsoft Corporation; Microsoft SQL Server; © Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;4"
                  description="Consumes data from SQL Server, OLE DB, ODBC, or Oracle, using the corresponding .NET Framework data provider. Use a Transact-SQL statement to define the result set. For example, extract data from SQL Server with the .NET Framework data provider for SQL Server."
                  name="Get CCFA Organization Location Data"
                  usesDispositions="true"
                  version="4">
                  <properties>
                    <property
                      dataType="System.String"
                      description="Specifies the SQL statement used by the component to extract data."
                      expressionType="Notify"
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select 
	null as MAAID
	, case	
		when la.Address1 is null then 
			case	
				when la.Address2 is not null then la.Address2
				else ''
			end 
		else la.Address1
	  end as address1
	, la.Address2 
	, la.City as CityTownName
	, la.RefStateID as StateCode
	--, (SELECT [USStateID] FROM [EDRMDM_ETL].[ref].[USState] where [FIPSStateCode] =  la.RefStateID) as StateCode	--Pull from ref.USState?
	, case	
		when la.RefCountryID is null then ''
		else la.RefCountryID
	  end as CountryCode
	--, case
	--	when len(la.RefCountryID) =3 then (SELECT [CountryID] FROM [EDRMDM_ETL].[ref].[Country] where [ISO3Code] = la.RefCountryID)
	--	else (SELECT [CountryID] FROM [EDRMDM_ETL].[ref].[Country] where [ISO2Code] = la.RefCountryID)
	--  end as CountryCode		--pull from ref.Country?
	, case	
		when la.PostalCode is null then ''
		else la.PostalCode
	  end as postalCode
	, la.Latitude 
	, la.Longitude 
	, (SELECT [CityTownID] FROM [EDRMDM_ETL].[ref].[CityTown] WHERE upper([CityTownName]) = upper(la.City)) as CityTownID --Pull from shared.City?
	, ol.OrganizationLocationID as CreatedBy
	, getDate() as AddressCreationDate
	, 'CCFA'  as ModifiedBy
	, getDate() as ModifiedDate
from [EDRStage].[EEC-CCFA].[OrganizationLocation] ol
left join [EDRStage].[EEC-CCFA].[LocationAddress] la
	on la.LocationAddressID = ol.LocationAddressID</property>
                    <property
                      dataType="System.Int32"
                      description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                      name="CommandTimeout">30</property>
                    <property
                      dataType="System.Boolean"
                      description="Indicates whether to allow non-string external columns to be implicitly converted to strings at the output columns."
                      name="AllowImplicitStringConversion">true</property>
